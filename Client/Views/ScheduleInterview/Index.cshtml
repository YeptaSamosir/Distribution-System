@{
    ViewData["Title"] = "Schedule Interview";
    Layout = "_LayoutApp";
}
<div class="right_col" role="main">
    <div class="row">
        <div class="col-12">
            <nav aria-label="breadcrumb">
                <ol class="breadcrumb">
                    <li class="breadcrumb-item"><a href="#">Home</a></li>
                    <li class="breadcrumb-item"><a href="~/admin">Admin</a></li>
                    <li class="breadcrumb-item active" aria-current="page">@ViewData["Title"]</li>
                </ol>
            </nav>
        </div>

        <div class="col-12">
            <h3>@ViewData["Title"]</h3>
        </div>
    </div>
    <div class="row">
        <div class="col-md-12 col-sm-12 ">
            <div class="x_panel">
                <div class="x_title">
                    <h2>Data Akun</h2>
                    <ul class="nav navbar-right panel_toolbox">
                        <li>
                            <a class="collapse-link"><i class="fa fa-chevron-up"></i></a>
                        </li>
                        <li class="dropdown">
                            <a href="#" class="dropdown-toggle" data-toggle="dropdown" role="button" aria-expanded="false"><i class="fa fa-wrench"></i></a>
                            <div class="dropdown-menu" aria-labelledby="dropdownMenuButton">
                                <a class="dropdown-item" href="#">Settings 1</a>
                                <a class="dropdown-item" href="#">Settings 2</a>
                            </div>
                        </li>
                        <li>
                            <a class="close-link"><i class="fa fa-close"></i></a>
                        </li>
                    </ul>
                    <div class="clearfix"></div>
                </div>
                <div class="x_content">
                    <div class="row">
                        <div class="col-sm-12">
                            <div class="card-box table-responsive">
                                <a type="button" class="btn btn-success" href="~/admin/scheduleinterview/create" >Add New Interview Schedule</a>

                                <table id="datatable-interview-schedule" class="table table-striped table-bordered" style="width:100%">
                                    <thead>
                                        <tr>
                                            <th width="1%">No</th>
                                            <th>Customer Name</th>
                                            <th>Job Title</th>
                                            <th>Location Interview</th>
                                            <th>Start Date</th>
                                            <th>End Date</th>
                                            <th width="10%">Action</th>
                                        </tr>
                                    </thead>
                                    <tbody>
                                    </tbody>
                                </table>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
<!-- /page content -->
<div class="modal fade" id="modalEdit" tabindex="-1" role="dialog" aria-labelledby="editLabel"
     aria-hidden="true">
    <div class="modal-dialog" role="document">
        <div class="modal-content">
            <div class="modal-header">
                <h5 class="modal-title" id="editLabel">Edit Schedule Interview</h5>
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                </button>
            </div>
            <form id="form-edit-schedule-interview" method="post">
                <div class="modal-body">
                    <div class="form-group">
                        <input id="scheduleInterviewID" type="hidden" value="">
                        <input id="candidateID" type="hidden" value="">
                        <input id="companyID" type="hidden" value="">
                        <input id="statusID" type="hidden" value="">

                        <label for="customerName">Customer Name</label>
                        <input id="customerName" type="text" class="form-control" value="">
                        <div class="invalid-feedback">
                            <span id="messageCustomerName"></span>
                        </div>

                        <label for="jobTitle">Job Title</label>
                        <input id="jobTitle" type="text" class="form-control" value="">
                        <div class="invalid-feedback">
                            <span id="messageJobTitle"></span>
                        </div>

                        <label for="locationInterview">Location Interview</label>
                        <input id="locationInterview" type="text" class="form-control" value="">
                        <div class="invalid-feedback">
                            <span id="messageLocationInterview"></span>
                        </div>

                        <label for="startDate">Start Date</label>
                        <input id="startDate" class="date-picker form-control" placeholder="dd-mm-yyyy" type="text" required="required" type="text" onfocus="this.type='date'" onmouseover="this.type='date'" onclick="this.type='date'" onblur="this.type='text'" onmouseout="timeFunctionLong(this)">
                        <div class="invalid-feedback">
                            <span id="messageLocationInterview"></span>
                        </div>

                        <label for="endDate">End Date</label>
                        <input id="endDate" class="date-picker form-control" placeholder="dd-mm-yyyy" type="text" required="required" type="text" onfocus="this.type='date'" onmouseover="this.type='date'" onclick="this.type='date'" onblur="this.type='text'" onmouseout="timeFunctionLong(this)">
                        <div class="invalid-feedback">
                            <span id="messageLocationInterview"></span>
                        </div>

                    </div>
                </div>
                <div class="modal-footer">
                    <button type="button" class="btn btn-secondary" data-dismiss="modal">Close</button>
                    <button type="submit" class="btn btn-success">Update</button>
                </div>
            </form>
        </div>
    </div>
</div>
