@{
    ViewData["Title"] = "Schedule Interview";
    Layout = "_LayoutApp";
}
 page content
<div class="right_col" role="main">
    <div class="row">
        <div class="col-12">
            <nav aria-label="breadcrumb">
                <ol class="breadcrumb">
                    <li class="breadcrumb-item"><a href="#">Home</a></li>
                    <li class="breadcrumb-item"><a href="~/admin">Admin</a></li>
                    <li class="breadcrumb-item active" aria-current="page">@ViewData["Title"]</li>
                </ol>
            </nav>
        </div>

        <div class="col-12">
            <h3>@ViewData["Title"]</h3>
        </div>
    </div>
    <div class="row">
        <div class="col-md-12 col-sm-12 ">
            <div class="x_panel">
                <div class="x_title">
                    <h2>Data Akun</h2>
                    <ul class="nav navbar-right panel_toolbox">
                        <li>
                            <a class="collapse-link"><i class="fa fa-chevron-up"></i></a>
                        </li>
                        <li class="dropdown">
                            <a href="#" class="dropdown-toggle" data-toggle="dropdown" role="button" aria-expanded="false"><i class="fa fa-wrench"></i></a>
                            <div class="dropdown-menu" aria-labelledby="dropdownMenuButton">
                                <a class="dropdown-item" href="#">Settings 1</a>
                                <a class="dropdown-item" href="#">Settings 2</a>
                            </div>
                        </li>
                        <li>
                            <a class="close-link"><i class="fa fa-close"></i></a>
                        </li>
                    </ul>
                    <div class="clearfix"></div>
                </div>
                <div class="x_content">
                    <div class="row">
                        <div class="col-sm-12">
                            <div class="card-box table-responsive">
                                <button type="button" class="btn btn-success" data-toggle="modal" data-target=".bs-example-modal-lg">Add New Interview Schedule</button>

                                <div id="modalCandidate" class="modal fade bs-example-modal-lg" tabindex="-1" role="dialog" aria-hidden="true">
                                    <div class="modal-dialog modal-lg">
                                        <div class="modal-content">

                                            <div class="modal-header">
                                                <h4 class="modal-title" id="myModalLabel">Add New Interview Schedule</h4>
                                                <button type="button" class="close" data-dismiss="modal">
                                                    <span aria-hidden="true">×</span>
                                                </button>
                                            </div>
                                            <form id="form-create-schedule-interview" method="post" novalidate>
                                                <div class="modal-body">
                                                    <div class="field item form-group">
                                                        <label class="col-form-label col-md-3 col-sm-3  label-align">Schedule Interview ID<span class="required">*</span></label>
                                                        <div class="col-md-6 col-sm-6">
                                                            <input class="form-control" id="inputScheduleInterviewID" data-validate-length-range="15" data-validate-words="2" name="interviewID" required="required" />
                                                            <div class="invalid-feedback">
                                                                <span id="messageScheduleInterviewID"></span>
                                                            </div>
                                                        </div>
                                                    </div>
                                                    <div class="field item form-group">
                                                        <label class="col-form-label col-md-3 col-sm-3  label-align">Candidate ID<span class="required">*</span></label>
                                                        <div class="col-md-6 col-sm-6">
                                                            <input class="form-control" id="inputCandidateID" data-validate-length-range="15" data-validate-words="2" name="candidateID" required="required" />
                                                            <div class="invalid-feedback">
                                                                <span id="messageCandidateID"></span>
                                                            </div>
                                                        </div>
                                                    </div>
                                                    <div class="field item form-group">
                                                        <label class="col-form-label col-md-3 col-sm-3  label-align">Company ID<span class="required">*</span></label>
                                                        <div class="col-md-6 col-sm-6">
                                                            <input class="form-control" id="inputCompanyID" data-validate-length-range="15" data-validate-words="2" name="companyID" required="required" />
                                                            <div class="invalid-feedback">
                                                                <span id="messageCompanyID"></span>
                                                            </div>
                                                        </div>
                                                    </div>
                                                    <div class="field item form-group">
                                                        <label class="col-form-label col-md-3 col-sm-3  label-align">Status ID<span class="required">*</span></label>
                                                        <div class="col-md-6 col-sm-6">
                                                            <input class="form-control" id="statusID" data-validate-length-range="15" data-validate-words="2" name="statusID" required="required" />
                                                            <div class="invalid-feedback">
                                                                <span id="messageStatusID"></span>
                                                            </div>
                                                        </div>
                                                    </div>
                                                    <div class="field item form-group">
                                                        <label class="col-form-label col-md-3 col-sm-3  label-align">Customer Name<span class="required">*</span></label>
                                                        <div class="col-md-6 col-sm-6">
                                                            <input class="form-control" id="inputCustomerName" data-validate-length-range="15" data-validate-words="2" name="namaCustomer" required="required" />
                                                            <div class="invalid-feedback">
                                                                <span id="messageCustomerName"></span>
                                                            </div>
                                                        </div>
                                                    </div>
                                                    <div class="field item form-group">
                                                        <label class="col-form-label col-md-3 col-sm-3  label-align">Job Title<span class="required">*</span></label>
                                                        <div class="col-md-6 col-sm-6">
                                                            <input class="form-control" id="inputJobTitle" data-validate-length-range="15" data-validate-words="1" name="jobTitle" required="required" />
                                                            <div class="invalid-feedback">
                                                                <span id="messageJobTitle"></span>
                                                            </div>
                                                        </div>
                                                    </div>
                                                    <div class="field item form-group">
                                                        <label class="col-form-label col-md-3 col-sm-3  label-align">Location Interview<span class="required">*</span></label>
                                                        <div class="col-md-6 col-sm-6">
                                                            <input class="form-control" id="inputLocationInterview" data-validate-length-range="15" data-validate-words="1" name="locationInterview" required="required" />
                                                            <div class="invalid-feedback">
                                                                <span id="messageLocationInterview"></span>
                                                            </div>
                                                        </div>
                                                    </div>
                                                    <div class="field item form-group">
                                                        <label class="col-form-label col-md-3 col-sm-3  label-align">Start Date<span class="required">*</span></label>
                                                        <div class="col-md-6 col-sm-6">
                                                            <input id="inputStartDate" class="date-picker form-control" placeholder="dd-mm-yyyy" type="text" required="required" type="text" onfocus="this.type='date'" onmouseover="this.type='date'" onclick="this.type='date'" onblur="this.type='text'" onmouseout="timeFunctionLong(this)">
                                                            <script>
                                                                function timeFunctionLong(input) {
                                                                    setTimeout(function () {
                                                                        input.type = 'text';
                                                                    }, 60000);
                                                                }
                                                            </script>
                                                            <div class="invalid-feedback">
                                                                <span id="messageStartDate"></span>
                                                            </div>
                                                        </div>
                                                    </div>
                                                    <div class="field item form-group">
                                                        <label class="col-form-label col-md-3 col-sm-3  label-align">End Date<span class="required">*</span></label>
                                                        <div class="col-md-6 col-sm-6">
                                                            <input id="inputEndDate" class="date-picker form-control" placeholder="dd-mm-yyyy" type="text" required="required" type="text" onfocus="this.type='date'" onmouseover="this.type='date'" onclick="this.type='date'" onblur="this.type='text'" onmouseout="timeFunctionLong(this)">
                                                            <script>
                                                                function timeFunctionLong(input) {
                                                                    setTimeout(function () {
                                                                        input.type = 'text';
                                                                    }, 60000);
                                                                }
                                                            </script>
                                                            <div class="invalid-feedback">
                                                                <span id="messageEndDate"></span>
                                                            </div>
                                                        </div>
                                                    </div>
                                                </div>
                                                <div class="modal-footer">
                                                    <button type="button" class="btn btn-secondary" data-dismiss="modal">Close</button>
                                                    <button type="submit" class="btn btn-success">Daftar Candidate</button>
                                                </div>
                                            </form>
                                        </div>
                                    </div>
                                </div>

                                <table id="datatable-interview-schedule" class="table table-striped table-bordered" style="width:100%">
                                    <thead>
                                        <tr>
                                            <th width="1%">No</th>
                                            <th>Customer Name</th>
                                            <th>Job Title</th>
                                            <th>Location Interview</th>
                                            <th>Start Date</th>
                                            <th>End Date</th>
                                            <th width="10%">Action</th>
                                        </tr>
                                    </thead>
                                    <tbody>
                                    </tbody>
                                </table>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
<!-- /page content -->
<div class="modal fade" id="modalEdit" tabindex="-1" role="dialog" aria-labelledby="editLabel"
     aria-hidden="true">
    <div class="modal-dialog" role="document">
        <div class="modal-content">
            <div class="modal-header">
                <h5 class="modal-title" id="editLabel">Edit Schedule Interview</h5>
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                </button>
            </div>
            <form id="form-edit-schedule-interview" method="post">
                <div class="modal-body">
                    <div class="form-group">
                        <input id="scheduleInterviewID" type="hidden" value="">
                        <input id="candidateID" type="hidden" value="">
                        <input id="companyID" type="hidden" value="">
                        <input id="statusID" type="hidden" value="">

                        <label for="customerName">Customer Name</label>
                        <input id="customerName" type="text" class="form-control" value="">
                        <div class="invalid-feedback">
                            <span id="messageCustomerName"></span>
                        </div>

                        <label for="jobTitle">Job Title</label>
                        <input id="jobTitle" type="text" class="form-control" value="">
                        <div class="invalid-feedback">
                            <span id="messageJobTitle"></span>
                        </div>

                        <label for="locationInterview">Location Interview</label>
                        <input id="locationInterview" type="text" class="form-control" value="">
                        <div class="invalid-feedback">
                            <span id="messageLocationInterview"></span>
                        </div>

                        <label for="startDate">Start Date</label>
                        <input id="startDate" class="date-picker form-control" placeholder="dd-mm-yyyy" type="text" required="required" type="text" onfocus="this.type='date'" onmouseover="this.type='date'" onclick="this.type='date'" onblur="this.type='text'" onmouseout="timeFunctionLong(this)">
                        <div class="invalid-feedback">
                            <span id="messageLocationInterview"></span>
                        </div>

                        <label for="endDate">End Date</label>
                        <input id="endDate" class="date-picker form-control" placeholder="dd-mm-yyyy" type="text" required="required" type="text" onfocus="this.type='date'" onmouseover="this.type='date'" onclick="this.type='date'" onblur="this.type='text'" onmouseout="timeFunctionLong(this)">
                        <div class="invalid-feedback">
                            <span id="messageLocationInterview"></span>
                        </div>

                    </div>
                </div>
                <div class="modal-footer">
                    <button type="button" class="btn btn-secondary" data-dismiss="modal">Close</button>
                    <button type="submit" class="btn btn-success">Update</button>
                </div>
            </form>
        </div>
    </div>
</div>
